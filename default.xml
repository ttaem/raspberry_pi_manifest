<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<remote                                                                      
		name="origin"                                                            
		fetch="ssh://git@github.com/"/>                                          
	<default
		revision="main"  
		remote="origin"  
		sync-j="4" />
	<project                                          
		groups="pi"
		sync-c="true"
		revision="rpi-5.10.y"
		path="linux"                            
		name="raspberrypi/linux"/>
	<project                                          
		groups="my"
		sync-c="true"
		revision="main"
		upstream="main"
		path="build"                                  
		name="ttaem/raspberry_pi_build">
	<copyfile src="top.mk" dest="Makefile"/>
	<copyfile src="top.sh" dest="Env.sh"/>
	</project>
</manifest>
